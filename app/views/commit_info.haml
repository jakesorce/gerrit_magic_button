.control-group
  %label.control-label{:for => "project"} Project:
  .controls
    %input#project{:readonly => "readonly", :name => "project", :value => "#{@project}"}/
.control-group
  %label.control-label{:for => "patchset"} Patchset:
  .controls
    %input#patchset{:readonly => "readonly", :name => "patchset", :value => "#{@patchset}"}/
    %a.gerrit_link{:href => "#{UrlHelpers.build_gerrit_url(@patchset)}", :target => '_blank'} View in Gerrit
